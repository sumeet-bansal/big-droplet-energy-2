        type: 'line',                                                               
        data: {                                                                          
                labels:["Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"], 
                datasets: [{                                                                   
                        label: "Access",                                           
                        data:[100,300,200,900,1200,1900,200],
                        borderColor: "#3e95cd",                                                    
                        fill: false                                                                 
                }]                                                                              
        },                                                                               
        options: {                                                                       
                title: {                                                                       
                display: true,                                                               
                text: 'Access per day'                                           
