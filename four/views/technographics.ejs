<script>

	var firefox = document.getElementById("firefox").innerHTML;
	var seamonkey = document.getElementById("seamonkey").innerHTML;
	var chrome = document.getElementById("chrome").innerHTML;
	var chromium = document.getElementById("chromium").innerHTML;
	var safari = document.getElementById("safari").innerHTML;
	var opera = document.getElementById("opera").innerHTML;
	var explorer = document.getElementById("explorer").innerHTML;
	var other = document.getElementById("other").innerHTML;


	var ctx = document.getElementById("browser");
	var myChart = new Chart(ctx, {
		type: 'pie',
		data: {
			datasets: [{
				data: [firefox, seamonkey, chrome, chromium, safari, opera, explorer, other],
				backgroundColor: [
					'rgba(255, 99, 132, 0.2)',
					'rgba(54, 162, 235, 0.2)',
					'rgba(255, 206, 86, 0.2)',
					'rgba(75, 192, 192, 0.2)',
					'rgba(153, 102, 255, 0.2)',
					'rgba(255, 159, 64, 0.2)',
					'rgba(154, 162, 135, 0.2)',
                                        'rgba(71, 62, 235, 0.2)'
				],
				borderColor: [
					'rgba(255,99,132,1)',
					'rgba(54, 162, 235, 1)',
					'rgba(255, 206, 86, 1)',
					'rgba(75, 192, 192, 1)',
