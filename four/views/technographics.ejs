					'rgba(255, 159, 64, 1)',
					'rgba(154, 162, 135, 1)',
					'rgba(71, 62, 235, 1)'
				],
				borderWidth: 1
			}],

			labels: [
				'Firefox',
				'Seamonkey',
				'Chrome',
				'Chromium',
				'Safari',
				'Opera',
				'Explorer',
				'Other'
			]
		},
		options: {
			startAngle: 0,
			animation: {
				animateRotate: true,
				animateScale: true
			}
		}
	});

	var mobile = document.getElementById("mobile").innerHTML;
	var desktop = document.getElementById("desktop").innerHTML;    

	var ctx1 = document.getElementById("device");
        var myChart = new Chart(ctx1, {
                type: 'pie',
                data: {
                        datasets: [{
                                data: [mobile, desktop],
                                backgroundColor: [
                                        'rgba(255, 99, 132, 0.2)',
                                        'rgba(54, 162, 235, 0.2)'
                                ],
                                borderColor: [
                                        'rgba(255,99,132,1)',
                                        'rgba(54, 162, 235, 1)'
                                ],
                                borderWidth: 1
                        }],

                        labels: [
                                'Mobile',
                                'Desktop'
                        ]
                },
                options: {
                        startAngle: 0,
                        animation: {
                                animateRotate: true,
                                animateScale: true
                        }
                }
        });

	var month = document.getElementById("month").innerHTML;

	var ctx2 = document.getElementById("month");                               
